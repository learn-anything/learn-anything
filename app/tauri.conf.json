    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "learn-anything-app",
  "version": "0.1.0",
  "identifier": "com.das.dev",
  "plugins": {},
  "app": {
    "windows": [
      {
        "transparent": false,
        "decorations": true,
        "fullscreen": false,
        "height": 600,
