{
  "name": "learn-anything",
  "scripts": {
    "dev": "bun web",
    "web": "cd web && bun dev",
    "web:build": "bun run --filter '*' build",
    "cli": "bun run --watch cli/run.ts",
    "seed": "bun --watch cli/seed.ts"
  },
  "workspaces": [
    "web"
  ],
  "dependencies": {
    "jazz-nodejs": "0.7.35-unique.2",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "bun-types": "^1.1.26"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "useTabs": true,
    "semi": false,
    "trailingComma": "none",
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "license": "MIT"
}
